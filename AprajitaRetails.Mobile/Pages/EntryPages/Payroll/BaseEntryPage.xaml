<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage x:Class="AprajitaRetails.Mobile.Pages.EntryPages.Payroll.BaseEntryPage"
             xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Background="{OnPlatform Default=White, WinUI=WhiteSmoke, MacCatalyst=WhiteSmoke}"
             xmlns:converters="clr-namespace:AprajitaRetails.Mobile.Converters;assembly=AprajitaRetails.Mobile"
             xmlns:dataForm="clr-namespace:Syncfusion.Maui.DataForm;assembly=Syncfusion.Maui.DataForm"
             xmlns:thickness ="clr-namespace:Microsoft.Maui;assembly=Microsoft.Maui"
              xmlns:system="clr-namespace:System;assembly=netstandard"
              xmlns:enty="clr-namespace:AprajitaRetails.Mobile.Views.Custom"
             xmlns:local="clr-namespace:AprajitaRetails.Mobile.Pages.EntryPages.Payroll">
    <ContentPage.Padding>
        <OnPlatform x:TypeArguments="thickness:Thickness">
            <On Value="-4,-4,-6,-6" Platform="MacCatalyst" />
            <On Value="-4,-4,-6,-6" Platform="WinUI" />
        </OnPlatform>
    </ContentPage.Padding>

    <ContentPage.Content>
        <enty:BaseEntryForm x:Name="FormData" />
    </ContentPage.Content>
</ContentPage>